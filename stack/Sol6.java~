//6. Balanced Parenthesis
// O(n) time 
import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Sol6{

	static void check(Stack st, char c){
		char x = (char)st.peek();
		if(c==')' && x =='(')
			st.pop();
		else if (c=='}' && x =='{')
			st.pop();
		else if(c==']' && x =='[')
			st.pop();  	
	}

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int t = in.nextInt();
        for(int a0 = 0; a0 < t; a0++){
            String s = in.next();
        }
	Stack st =new Stack();
	for(int i=0;i<s.length();i++){
		if(s.charAt(i)=='('||s.charAt(i)=='{'||s.charAt(i)=='['){
			st.push(s.charAt(i));		
		}
		else
			check(st,s.charAt(i));	
	}
	if(st.isEmpty())
		System.out.println("YES");
	else
		System.out.println("NO");
    }
}

